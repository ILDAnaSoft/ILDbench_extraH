		<parameter name="recParticleIn" type="string" lcioInType="ReconstructedParticle"> PFOWoIsoleps </parameter>
		<!--verbosity level of this processor ("DEBUG0-4,MESSAGE0-4,WARNING0-4,ERROR0-4,SILENT")-->
